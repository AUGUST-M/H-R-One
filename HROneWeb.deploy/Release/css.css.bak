table
{
	text-align: left;
	font-family: Arial, Helvetica, sans-serif;
}

.button
{
	background-color: #fe6a10;
	color: #ffffff;
	border: solid 1px #cccccc;
	margin-top:2px;
	margin-bottom:2px;
}

.header_text_blue
{
	font-size: 18px;
	line-height: normal;
	font-weight: normal;
	color: #1D1264;
	font-family: Arial, Helvetica, sans-serif;
}
.inside_text_black
{
	font-size: 14px;
	line-height: normal;
	font-weight: normal;
	color: #000000;
	font-family: Arial, Helvetica, sans-serif;
}
.inside_text_greyform
{
	font-size: 12px;
	line-height: normal;
	font-weight: normal;
	color: #A7A7A7;
	font-family: Arial, Helvetica, sans-serif;
}
.inside_text_blackform
{
	font-size: 12px;
	line-height: normal;
	font-weight: normal;
	color: #000000;
	font-family: Arial, Helvetica, sans-serif;
}
.inside_text_blueform
{
	font-size: 12px;
	line-height: normal;
	font-weight: normal;
	color: #1D1264;
	font-family: Arial, Helvetica, sans-serif;
}
.inside_text_blackformdata
{
	font-size: 11px;
	line-height: normal;
	font-weight: normal;
	color: #000000;
	font-family: Arial, Helvetica, sans-serif;
}
.inside_text_whiteform
{
	font-size: xx-small;
