	font-family: Arial, Helvetica, sans-serif;
}
.button_default
{
	background-color: #DDE7EF;
}
table.table_border
{
	border-collapse: collapse;
	border-style: double;
	background-color: #FFFFFF;
	border-color: #99CCCC;
}
td.button_border
{
	border-style: solid;
	border-width: 1px;
}


.pm_search_header
{
	font-size: 11px;
	color: #000000;
	font-family: "Arial", "Helvetica", "sans-serif";

}
.pm_search
{
	font-size: 11px;
	color: #000000;
	font-family: "Arial", "Helvetica", "sans-serif";

}

.pm_section
{

	border-collapse: collapse;
	border: solid 0px #cccccc;
	padding: 3px 3px 3px 3px ;
	margin-top: 0px;
	margin-bottom: 10px;
}
.pm_section_title
{
	font-family: "Arial", "Helvetica", "sans-serif";
	font-size: 16px;
	font-weight: bold;
	color: #FF9900;
	text-decoration: none;
	border-top: 1px solid;
	border-bottom: 1px solid;
	padding: 3px 5px 3px 1px;
	margin-bottom: 15px;
	margin-top: 10px;
	vertical-align: middle;
	background-repeat: repeat-x;
	background-position: top;
	
}


.pm_list_footer,
.pm_list_pagenav
{
    background-color:#dddddd;
	font-size: 12px;
	line-height: normal;
	font-weight: normal;
	font-family: Arial, Helvetica, sans-serif;
}
.pm_list_pagenav
{
	color: #A7A7A7;
}
.pm_link_pagenav
{
	color: #000000;
	
}

.pm_list_section
{
	border-collapse: collapse;
	border: solid 1px #cccccc;
	font-size: 12px;
	background-repeat: repeat-x;
	background-position: top;
}

.pm_list_header
{
	border: solid 1px #9FC6C8; 
	background-color:#9FC6C8;
	font-size: 12px;
	color: #ffffff;
	font-family: Arial, Helvetica, sans-serif;
}

.pm_list_header a{
	text-decoration: none;
	color: #ffffff;
}
.pm_list_header a:hover 
{
	text-decoration: underline;
}

.pm_list, 
.pm_list_alt_row, 
.pm_list_edit
{
	border: solid 1px #cccccc; 
	font-size: 11px;
	background-color:#FFFFFF;
	color: #000000;
	font-family: Arial, Helvetica, sans-serif;
}
.pm_list select,
.pm_list_alt_row select,
.pm_list_edit select,
.pm_list input,
.pm_list_alt_row input,
.pm_list_edit input
{
	font-size: 11px;
	font-family: Arial, Helvetica, sans-serif;
}
.pm_list_alt_row
{
	background-color:#f0f0f0;
}
.pm_list_edit
{
	background-color: #ffff77;
}
.pm_field_section
{
	border-collapse: collapse;
	border: solid 1px #cccccc;
}

.pm_field_title
{
	padding-right:5px;
	padding-left:5px;
	padding-top:1px;
	padding-bottom:1px;
	border: solid 1px #aaaaaa;
	background-color: #aaaaaa;
	color: #000000;
	font-family: Arial, Helvetica, sans-serif;
	font-size: 12px;
	font-weight: bold;
}

.pm_field_header
{
	padding-right:5px;
	padding-left:5px;
	padding-top:1px;
	padding-bottom:1px;
	border: solid 1px #cccccc;
	background-color: #cccccc;
	color: #000000;
	font-family: Arial, Helvetica, sans-serif;
	font-size: 12px;
	text-align: Left;
}

.pm_field
{
	padding-right:5px;
	padding-left:5px;
	border: solid 1px #cccccc;
	background-color: #ffffff;
	font-size: 12px;
	line-height: normal;
	font-weight: normal;
	color: #000000;
	font-family: Arial, Helvetica, sans-serif;
}


.pm_submenu_section 
{
	border-collapse: collapse;
	border: solid 1px #346667;
	background-color:#346667;
	width: 160px;
	margin: 10px 10px  10px  0px ;
}

.pm_submenu_item 
{
	padding: 5px 10px 5px 10px;
}

.pm_submenu_item a{
	font-family: "Arial", "Helvetica", "sans-serif";
	font-size: 12px;
	text-decoration: none;
	color: #ffffff;
}
.content {
	font-family: "Arial", "Helvetica", "sans-serif";
	font-size: x-small;
	color: #000000;
}


.title01 {
	font-family: "Arial", "Helvetica", "sans-serif";
	font-size: small;

	font-weight: bold;
	color: #FF9900;
	text-decoration: none;
}
.copyright {
	font-family: "Arial", "Helvetica", "sans-serif";
	font-size: 10px;
	color: #666666;
	text-decoration: none;
}

.pm_button_section
{
	font-size: 11px;
	background-color: transparent;
	color: #000000;
	font-family: Arial, Helvetica, sans-serif;
}
.inside_text_blackform
{
	font-size: 12px;
	line-height: normal;
	font-weight: normal;
	color: #000000;
	font-family: Arial, Helvetica, sans-serif;
}

.pm_page_title
{
	font-family: "Arial", "Helvetica", "sans-serif";
	font-size: 24px;
	font-weight: bold;
	color: #FF9900;
	text-decoration: none;
	padding-top:3px;
	padding-bottom:10px;
	
}
.pm_required
{
	background-color: #ffCC88;
}
body.mainbody
{
	background-image: url(./images/header/backfull.gif);
	margin-top: 0px;
	margin-left: 0px;
	margin:0px;
	padding:0px;
	font-size: 12px;
	font-family: Arial, Helvetica, sans-serif ;
}

